{
  "name": "oneshot",
  "version": "1.0.0",
  "type": "module",
"scripts": {
  "dev": "concurrently \"vite\" \"node --loader ts-node/esm src/backend/index.ts\"",
  "frontend": "vite",
  "backend": "node --loader ts-node/esm src/backend/index.ts",
  "build": "vite build",
  "kill": "pkill -f vite || true && pkill -f ts-node || true"
},
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "postcss": "^8.4.27",
    "postgres": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2",
    "vite": "^6.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}

    "@types/pg": "^8.11.14",
    "drizzle-kit": "^0.32.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "react-qr-code": "^2.0.15",
    "vcf": "^2.1.2"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",

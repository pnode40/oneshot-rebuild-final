{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:server": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev:server\"",
    "migrate:generate": "drizzle-kit generate",
    "migrate:push": "drizzle-kit push",
    "migrate": "ts-node server/src/db/migrate.ts",
    "migrate:standard": "ts-node server/src/db/standard-migrate.ts",
    "migrate:direct": "ts-node server/src/db/custom-migrate.ts",
    "migrate:validate": "ts-node server/src/db/validate-migrations.ts",
    "migrate:rebuild": "ts-node server/src/db/rebuild-migration-state.ts",
    "migrate:rebuild-and-validate": "npm run migrate:rebuild && npm run migrate:validate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:coverage": "echo \"Test coverage not configured yet\" && exit 1",
    "type-check": "cd server && npx tsc --noEmit",
    "lint": "echo \"Linting not configured yet\" && exit 0",
    "autonomous:init": "node scripts/context-engine.js init",
    "autonomous:status": "node scripts/context-engine.js status",
    "autonomous:track": "node scripts/context-engine.js track-decision",
    "autonomous:metrics": "node scripts/context-engine.js update-metrics",
    "autonomous:control": "bash scripts/autonomous-dev.sh"
  },
